{
  "name": "transax",
  "version": "2.0.0-alpha.7",
  "description": "Internationalization",
  "type": "module",
  "scripts": {
    "build": "npm run build:parsers && npm run build:release && npm run build:docs",
    "build:parsers": "esrun bin/build-parsers.ts",
    "build:release": "esrun bin/build-release.ts",
    "build:docs": "typedoc && NODE_OPTIONS=--openssl-legacy-provider vuepress build docs",
    "docs": "typedoc && NODE_OPTIONS=--openssl-legacy-provider vuepress dev docs",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.ts",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --config jest.config.ts"
  },
  "typedocMain": "./index.ts",
  "module": "./esm/index.js",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "esm/**/*",
    "cjs/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TonyBogdanov/transax.git"
  },
  "author": "Tony Bogdanov <support@tonybogdanov.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TonyBogdanov/transax/issues"
  },
  "homepage": "https://github.com/TonyBogdanov/transax#readme",
  "devDependencies": {
    "@digitak/esrun": "^3.2.17",
    "@jest/globals": "^29.3.1",
    "@types/chai": "^4.3.4",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "browserify": "^17.0.0",
    "jest": "^29.3.1",
    "npm": "^9.3.1",
    "peggy": "^2.0.1",
    "rimraf": "^4.1.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-pegjs": "^3.0.0",
    "typedoc": "^0.23.28",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4",
    "uglify-js": "^3.17.4",
    "vue-tabs-component": "^1.5.0",
    "vuepress": "^1.9.8",
    "vuepress-plugin-tabs": "^0.3.0",
    "fsmonitor": "^0.2.4"
  },
  "dependencies": {
    "glob-promise": "^6.0.2"
  }
}
